clean_scintilla: 
	cmd /c del /f $(OBJ_SCINTILLA) $(OUT_SCINTILLA)
	cmd /c rd devel32
	cmd /c rd $(OBJDIR_SCINTILLA)\\sdk\\wxscintilla\\src
	cmd /c rd $(OBJDIR_SCINTILLA)\\sdk\\wxscintilla\\src\\scintilla\\lexers
	cmd /c rd $(OBJDIR_SCINTILLA)\\sdk\\wxscintilla\\src\\scintilla\\src
	cmd /c rd $(OBJDIR_SCINTILLA)\\sdk\\wxscintilla\\src\\scintilla\\lexlib

before_sdk: 
	$(TARGET_OBJECT_DIR)\autorevision +wx +int +t .. include/autorevision.h
	cmd /c if not exist devel32 md devel32
	cmd /c if not exist $(OBJDIR_SDK)\\sdk\\scripting\\bindings md $(OBJDIR_SDK)\\sdk\\scripting\\bindings
	cmd /c if not exist $(OBJDIR_SDK)\\sdk md $(OBJDIR_SDK)\\sdk
	cmd /c if not exist $(OBJDIR_SDK)\\sdk\\mozilla_chardet\\src md $(OBJDIR_SDK)\\sdk\\mozilla_chardet\\src

after_sdk: 
	cmd /c if not exist devel32\share\CodeBlocks mkdir devel32\share\CodeBlocks
	zip -jq9 devel32\share\CodeBlocks\manager_resources.zip sdk\resources\*.xrc
	cmd /c "cd sdk\resources & zip -0 -q ..\..\devel32\share\CodeBlocks\manager_resources.zip images\*.png images\8x8\*.png images\10x10\*.png images\12x12\*.png images\16x16\*.png images\20x20\*.png images\24x24\*.png images\28x28\*.png images\32x32\*.png images\40x40\*.png images\48x48\*.png images\56x56\*.png images\64x64\*.png images\svg\*.svg"

sdk: before_sdk out_sdk after_sdk

out_sdk: before_sdk $(OBJ_SDK) $(DEP_SDK)
	$(LD) -shared $(LIBDIR_SDK) $(OBJ_SDK)  -o $(OUT_SDK) $(LDFLAGS_SDK) $(LIB_SDK)

