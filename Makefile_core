before_tinyxml: 
	cmd /c if not exist devel32 md devel32
	cmd /c if not exist $(OBJDIR_TINYXML)\\base\\tinyxml md $(OBJDIR_TINYXML)\\base\\tinyxml

after_tinyxml: 

tinyxml: before_tinyxml out_tinyxml after_tinyxml

out_tinyxml: before_tinyxml $(OBJ_TINYXML) $(DEP_TINYXML)
	$(AR) rcs $(OUT_TINYXML) $(OBJ_TINYXML)

$(OBJDIR_TINYXML)\\base\\tinyxml\\tinyxmlparser.o: base\\tinyxml\\tinyxmlparser.cpp
	$(CXX) $(CFLAGS_TINYXML) $(INC_TINYXML) -c base\\tinyxml\\tinyxmlparser.cpp -o $(OBJDIR_TINYXML)\\base\\tinyxml\\tinyxmlparser.o

