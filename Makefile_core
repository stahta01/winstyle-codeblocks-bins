before_to_do: 
	cmd /c if not exist devel32\\share\\CodeBlocks\\plugins md devel32\\share\\CodeBlocks\\plugins
	cmd /c if not exist $(OBJDIR_TO_DO)\\plugins\\todo md $(OBJDIR_TO_DO)\\plugins\\todo

after_to_do: 
	cmd /c if not exist devel32\share\CodeBlocks mkdir devel32\share\CodeBlocks
	zip -jq9 devel32\share\CodeBlocks\todo.zip plugins\todo\resources\manifest.xml plugins\todo\resources\*.xrc

to_do: before_to_do out_to_do after_to_do

out_to_do: before_to_do $(OBJ_TO_DO) $(DEP_TO_DO)
	$(LD) -shared $(LIBDIR_TO_DO) $(OBJ_TO_DO)  -o $(OUT_TO_DO) $(LDFLAGS_TO_DO) $(LIB_TO_DO)

