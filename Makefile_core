before_abbreviations: 
	cmd /c if not exist devel32\\share\\CodeBlocks\\plugins md devel32\\share\\CodeBlocks\\plugins
	cmd /c if not exist $(OBJDIR_ABBREVIATIONS)\\plugins\\abbreviations md $(OBJDIR_ABBREVIATIONS)\\plugins\\abbreviations

after_abbreviations: 
	cmd /c if not exist devel32\share\CodeBlocks mkdir devel32\share\CodeBlocks
	zip -jq9 devel32\share\CodeBlocks\abbreviations.zip plugins\abbreviations\resources\manifest.xml plugins\abbreviations\resources\*.xrc

abbreviations: before_abbreviations out_abbreviations after_abbreviations

out_abbreviations: before_abbreviations $(OBJ_ABBREVIATIONS) $(DEP_ABBREVIATIONS)
	$(LD) -shared $(LIBDIR_ABBREVIATIONS) $(OBJ_ABBREVIATIONS)  -o $(OUT_ABBREVIATIONS) $(LDFLAGS_ABBREVIATIONS) $(LIB_ABBREVIATIONS)

$(OBJDIR_ABBREVIATIONS)\\plugins\\abbreviations\\abbreviationsconfigpanel.o: plugins\\abbreviations\\abbreviationsconfigpanel.cpp
