	$(LD) $(LIBDIR_AUTOREVISION) -o $(OUT_AUTOREVISION) $(OBJ_AUTOREVISION)  $(LDFLAGS_AUTOREVISION) $(LIB_AUTOREVISION)

$(OBJDIR_AUTOREVISION)\\build_tools\\autorevision\\autorevision.o: build_tools\\autorevision\\autorevision.cpp
	$(CXX) $(CFLAGS_AUTOREVISION) $(INC_AUTOREVISION) -c build_tools\\autorevision\\autorevision.cpp -o $(OBJDIR_AUTOREVISION)\\build_tools\\autorevision\\autorevision.o

