clean_sdk: 
	cmd /c del /f $(OBJ_SDK) $(OUT_SDK)
	cmd /c rd devel32
	cmd /c rd $(OBJDIR_SDK)\\sdk\\scripting\\bindings
	cmd /c rd $(OBJDIR_SDK)\\sdk
	cmd /c rd $(OBJDIR_SDK)\\sdk\\mozilla_chardet\\src

before_src: 
	cmd /c if exist devel32\exchndl.dll del devel32\exchndl.dll
	cmd /c if not exist devel32 md devel32
	cmd /c if not exist $(OBJDIR_SRC)\\src md $(OBJDIR_SRC)\\src
	cmd /c if not exist $(OBJDIR_SRC)\\src\\resources md $(OBJDIR_SRC)\\src\\resources

after_src: 
	cmd /c if not exist devel32\share\CodeBlocks mkdir devel32\share\CodeBlocks
	zip -jq9 devel32\share\CodeBlocks\resources.zip src\resources\*.xrc
	zip -jq9 devel32\share\CodeBlocks/start_here.zip src\resources\start_here\*.html src\resources\start_here\*.png
	cmd /c "cd src\resources & zip -0 -q ..\..\devel32\share\CodeBlocks\resources.zip images\*.png images\16x16\*.png"
	cmd /c if not exist devel32\exchndl.dll if exist exchndl\win32\bin\*.dll xcopy /Y exchndl\win32\bin\*.dll devel32\
	cmd /c if not exist devel32\symsrv.yes if exist exchndl\win32\bin\symsrv.yes xcopy /Y exchndl\win32\bin\symsrv.yes devel32\

src: before_src out_src after_src

